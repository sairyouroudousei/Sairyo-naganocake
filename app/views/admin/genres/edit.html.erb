<h1>Admin::Genres#edit</h1>
<p>Find me in app/views/admin/geres/edit.html.erb</p>
<h1>ジャンル編集</h1>
<%= form_with model: @genre, url: admin_genre_path(@genre), method: :patch, local: true do |f| %>
  <p>ジャンル名: <%= f.text_field :name %></p>
  <%= f.submit '更新' %>
<% end %>
<%= link_to '戻る', admin_genres_path %>