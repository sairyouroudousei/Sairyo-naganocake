<h1>Admin::Gernes#index</h1>
<p>Find me in app/views/admin/geres/index.html.erb</p>
<h1>ジャンル一覧</h1>
<%= form_with model: @genre, url: admin_genres_path, local: true do |f| %>
  <p>ジャンル名: <%= f.text_field :name %></p>
  <%= f.submit '追加' %>
<% end %>

<ul>
  <% @genres.each do |genre| %>
    <li>
      <%= genre.name %> |
      <%= link_to '編集', edit_admin_genre_path(genre) %>
    </li>
  <% end %>
</ul>